        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            @Html.LabelFor(model => model.CharacterName, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10" style="display: inline-flex">
                @Html.EditorFor(model => model.CharacterName, new { htmlAttributes = new { @class = "form-control", @Value = "", id = "randName" } })
